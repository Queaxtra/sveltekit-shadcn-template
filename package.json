{
	"name": "sveltekit-shadcn-template",
	"description": "A production-ready SvelteKit template featuring Shadcn UI, Tailwind CSS, and built-in i18n support. Perfect foundation for scalable, modern, and multilingual web applications.",
	"private": true,
	"version": "0.1.0-beta",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
